<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
	<item android:id="@android:id/background">
	    <shape>
	        <corners
	            android:radius="5dp"
	            />

	        <gradient
	            android:startColor="@color/bg.progress.score"
	            android:centerY="0.8"
	            android:endColor="@color/bg.progress.score"
	            android:angle="270"
	            />

		        <stroke
		            android:width="2dp"
		            android:color="#33FFFFFF"
		            />

	    </shape>
	</item>

	<item android:id="@android:id/progress">
	    <clip>
	        <shape>
		        <corners
		            android:radius="5dp"
		            />

		        <gradient
		            android:startColor="@color/bg.progress.score.bad"
		            android:centerColor="@color/bg.progress.score.bad"
		            android:centerY="0.8"
		            android:endColor="@color/bg.progress.score.badend"
		            android:angle="270"
		            />

		        <stroke
		            android:width="2dp"
		            android:color="#33FFFFFF"
		            />
	        </shape>
	    </clip>
	</item>

</layer-list>