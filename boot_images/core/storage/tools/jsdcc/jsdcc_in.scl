RAM CODE_START_ADDR CODE_SIZE
{
    CODE CODE_START_ADDR 
    {
      jsdcc_start.o (StartHere, +FIRST)
      jsdcc_main.o (+RO)
      * (+RO)
    }

    //  This block is for the RW data
    //
    APP_RAM +0x0
    {
      * (+RW)
    }
    
    //  This block is for the zero init data
    //
    APP_RAM_ZI +0x0
    {
      * (+ZI)
    }
}

