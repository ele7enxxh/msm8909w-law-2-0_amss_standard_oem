import os.path
Import('env')

UNIT_ROOT = env.GetBuildPath(Dir('..'))
INCLUDE_DIRS = [os.path.normpath(os.path.join(UNIT_ROOT, subcomponent))
                  for subcomponent in
                    ['cdma/inc',
                     'common/inc',
                     'gnss/inc',
                     'gsm/inc',
                     'lte/inc',
                     'tdscdma/inc',
                     'wcdma/inc']
                  ]

env.PublishPublicApi('RF_CARD__CONFIGURABLE_TEST_CARD', INCLUDE_DIRS)

