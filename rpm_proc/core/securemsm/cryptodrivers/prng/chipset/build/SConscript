Import('env')

env = env.Clone()

# set default script name
script_fname = "${CHIPSET}.SConscript"

# change name for other targets that migh start with q, etc
if env['MSM_ID'] in ['xxxx']:
   script_fname = "xxxx.SConscript"

#-------------------------------------------------------------------------------
# Load sub scripts
#-------------------------------------------------------------------------------
env.SConscript(script_fname, exports='env')



