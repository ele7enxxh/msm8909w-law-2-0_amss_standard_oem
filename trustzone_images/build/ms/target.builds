define REGION5_SIZE 0x002C000

# EBI1 ram 1GB from  (EBI1 CS0 start addr)
# ** DO NOT ** change the region addresses
# Boot Code assumes REGION6_BASE to lie
# in ebi1 memory region
define REGION6_BASE 0x80000000
define REGION6_SIZE 0x40000000

# RPM Message RAM 16KB from 0x00104000
# ** DO NOT ** change the region addresses
# Boot Code assumes REGION7_BASE to lie
# in Code Ram
define REGION9_BASE 0x00108000
define REGION9_SIZE 0x5fff

# SPS Buffer RAM 16KB from 0x12040000
# ** DO NOT ** change the region addresses
# Boot Code assumes REGION11_BASE to lie
# in SPS Buffer Ram
define REGION10_BASE 0x12040000
define REGION10_SIZE 0x4000

# SPS Pipe Memory 32KB from 0x12800000
# ** DO NOT ** change the region addresses
# Boot Code assumes REGION11_BASE to lie
# in SPS Pipe Memory
define REGION11_BASE 0x12800000
define REGION11_SIZE 0x8000


# MDSP Ram A 32KB from 0x09000000
# ** DO NOT ** change the region addresses
# Boot Code assumes REGION12_BASE to lie
# in MDSP Ram A
define REGION12_BASE 0x09000000
define REGION12_SIZE 0x8000

# MDSP Ram B 32KB from 0x09200000
# ** DO NOT ** change the region addresses
# Boot Code assumes REGION13_BASE to lie
# in MDSP Ram B
define REGION13_BASE 0x09200000
define REGION13_SIZE 0x8000

# MDSP Ram C 62KB from 0x09400000
# ** DO NOT ** change the region addresses
# Boot Code assumes REGION14_BASE to lie
# in MDSP Ram C
define REGION14_BASE 0x09400000
define REGION14_SIZE 0xF800

# MM GMEM 512kb from 0x2E030000
# ** DO NOT ** change the region addresses
# Boot Code assumes REGION15_BASE to lie
# in Multimedia GMEM
define REGION15_BASE 0x2E030000
define REGION15_SIZE 0x00080000

# Array of Memory regions to be dumped by RAM Dump Feature
# ** This array is used by RAM dump via mem-debug utility, **
# ** as well as SD-dump on boot using cookie               **
# New regions should be added here to this array, before the
# NULL last entry.
# Regions that span multiple chips, in which you wish to dump
# CS0 and CS1 separately will need logic added to
# 'dload_debug_8960.c', (i.e. EB1 CS0/CS1, SMI CS0/CS1),
# all other regions may be added to this array.

# Element format: {"Region Base Address","Region Size in Bytes","Friendly Region Name String","Output Filename"}
define MEMORY_REGION1 {REGION1_BASE, REGION1_SIZE, " System IMEM-A", "IMEM_A.BIN"}
define MEMORY_REGION2 {REGION2_BASE, REGION2_SIZE, " MM IMEM region", "MM_IMEM.BIN"}
define MEMORY_REGION3 {REGION3_BASE, REGION3_SIZE, "RPM Code RAM region", "CODERAM.BIN"}

define MEMORY_REGION5 {REGION5_BASE, REGION5_SIZE, "SPS RAM region", "SPS_RAM.BIN"}

define MEMORY_REGION8 {REGION8_BASE, REGION8_SIZE, " System IMEM-C", "IMEM_C.BIN"}
define MEMORY_REGION9 {REGION9_BASE, REGION9_SIZE, "RPM Msg RAM region", "RPM_MSG.BIN"}
define MEMORY_REGION10 {REGION10_BASE, REGION10_SIZE, "SPS Buff RAM region", "SPS_BUFF.BIN"}
define MEMORY_REGION11 {REGION11_BASE, REGION11_SIZE, "SPS Pipe Mem region", "SPS_PIPE.BIN"}
define MEMORY_REGION12 {REGION12_BASE, REGION12_SIZE, "MDSP Ram A region", "MDSP_A.BIN"}
define MEMORY_REGION13 {REGION13_BASE, REGION13_SIZE, "MDSP Ram B region", "MDSP_B.BIN"}
define MEMORY_REGION14 {REGION14_BASE, REGION14_SIZE, "MDSP Ram C region", "MDSP_C.BIN"}
define MEMORY_REGION_LAST {NULL, NULL, NULL, NULL}

# Memory Region Table note: Must be on one line as '\' operator not defined in .builds environment
define MEMORY_REGION_TABLE MEMORY_REGION1,MEMORY_REGION8,MEMORY_REGION2,MEMORY_REGION3,MEMORY_REGION9,MEMORY_REGION5,MEMORY_REGION10,MEMORY_REGION11,MEMORY_REGION12,MEMORY_REGION13,MEMORY_REGION14,MEMORY_REGION_LAST

