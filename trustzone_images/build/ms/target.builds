define CONVERT_TO_MB_SHIFT 20

define SD_PATH "/mmc1/"

# memory dump regions

# Boot ROM 128KB from 0x0
# ** DO NOT ** change the region addresses
# Boot Code assumes REGION0_BASE to lie
# in ROM
define REGION0_BASE 0x0
define REGION0_SIZE 0x0020000

# Q6 TCM 256KB from 0xFE800000
# ** DO NOT ** change the region addresses
# Boot Code assumes REGION1_BASE to lie
# in Q6 TCM
define REGION1_BASE 0xFE800000
define REGION1_SIZE 0x00040000

# System IMEM 16KB from 0x2B000000
# ** DO NOT ** change the region addresses
# Boot Code assumes REGION2_BASE to lie
# in System IMEM
define REGION2_BASE 0x2B000000
define REGION2_SIZE 0x4000

# RPM Code RAM 144KB from 0x20000
# ** DO NOT ** change the region addresses
# Boot Code assumes REGION3_BASE to lie
# in Code Ram
define REGION3_BASE SCL_RPM_CODE_RAM_BASE
define REGION3_SIZE SCL_RPM_CODE_RAM_SIZE

# EBI1 ram 128MB from  (EBI1 CS0 start addr)
# ** DO NOT ** change the region addresses
# Boot Code assumes REGION4_BASE to lie


# in ebi1 memory region
define REGION4_BASE 0x40000000
define REGION4_SIZE 0x08000000

# RPM Message RAM 16KB from 0x00104000
# ** DO NOT ** change the region addresses
# Boot Code assumes REGION5_BASE to lie
# in Code Ram
define REGION5_BASE 0x00104000

define REGION5_SIZE 0x4000


# Array of Memory regions to be dumped by RAM Dump Feature
# ** This array is used by RAM dump via mem-debug utility, **
# ** as well as SD-dump on boot using cookie               **
# New regions should be added here to this array, before the
# NULL last entry.
# Regions that span multiple chips, in which you wish to dump
# CS0 and CS1 separately will need logic added to
# 'dload_debug_9x15.c',
# all other regions may be added to this array.

# Element format: {"Region Base Address","Region Size in Bytes","Friendly Region Name String","Output Filename"}
define MEMORY_REGION1 {REGION1_BASE, REGION1_SIZE, "Q6 TCM region", "Q6_TCM.BIN"}
define MEMORY_REGION2 {REGION2_BASE, REGION2_SIZE, "System IMEM region", "SYS_IMEM.BIN"}
define MEMORY_REGION3 {REGION3_BASE, REGION3_SIZE, "RPM Code RAM region", "CODERAM.BIN"}

define MEMORY_REGION4 {REGION4_BASE, REGION4_SIZE, "EBI 1 region", "EBI1.BIN"}

define MEMORY_REGION5 {REGION5_BASE, REGION5_SIZE, "RPM msg region", "RPM_MSG.BIN"}
define MEMORY_REGION_LAST {NULL, NULL, NULL, NULL}

# Memory Region Table note: Must be on one line as '\' operator not defined in .builds environment
define MEMORY_REGION_TABLE MEMORY_REGION1,MEMORY_REGION2,MEMORY_REGION3,MEMORY_REGION4,MEMORY_REGION5,MEMORY_REGION_LAST

