HAL_PLAT := 8660
BUILD_ASIC := 8660
BUILDID := $(BUILD:%D=%)
PLATFORM := amss
NUM_THREADS := 8
VERBOSE := 0
TARGET_FAMILY := 8660
BUILD_MSM_ID := 8660
BUILD_LC := $(shell echo $(BUILD) | tr A-Z a-z)
CHIPSET := msm8660


ifeq ($(IMAGE), MODEM_PROC)
