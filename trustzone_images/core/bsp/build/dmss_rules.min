$(STM_OBJDIR)/%.smt: %.smf
	@echo ------------------------------------------------------------------------
	@echo STATE MACHINE $*
	$(CC) $(CFLAGS) $(DMSS_CFLAGS)  $(ALL_INCLUDES) $(QCC_PATHS) -I"$(ARMINC)" $(BUILD_VERSION) $(APPFLAGS) $(MSG_BT_SSID) -E $< | $(SMCOMP) -t -b $* -d $(STM_OBJDIR)
