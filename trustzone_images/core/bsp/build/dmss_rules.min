	@echo Preprocess $< into $@
	$(CC) -E $(CFLAGS) $(DMSS_CFLAGS)  $(ALL_INCLUDES) $(DIRNAME) $(QCC_PATHS) -I"$(ARMINC)" $(BUILD_VERSION) $(APPFLAGS) $(MSG_BT_SSID) $(OBJ_CMD) $@ $<
